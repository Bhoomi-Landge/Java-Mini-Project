<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.firstproject.FarmersDetails">


    <VBox alignment="CENTER" prefHeight="446.0" prefWidth="1000.0" spacing="20.0" style="-fx-background-color: #F4711E;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
        <TableView fx:id="table_users" layoutX="124.0" layoutY="43.0" prefHeight="266.0" prefWidth="500.0">
            <columns>
                <TableColumn fx:id="fullName" onEditCommit="#oneditchange" prefWidth="127.0" text="Full Name" />
                <TableColumn fx:id="storeName" minWidth="0.0" prefWidth="115.0" text="Storage Name" />
                <TableColumn fx:id="compartNo" minWidth="0.0" prefWidth="126.0" text="Compartment No" />
                <TableColumn fx:id="spaceOccu" minWidth="0.0" prefWidth="108.0" text="Space Occupied" />
                <TableColumn fx:id="fromDate" minWidth="0.0" prefWidth="121.0" text="From Date" />
                <TableColumn fx:id="toDate" minWidth="0.0" prefWidth="127.0" text="To date" />
            <TableColumn fx:id="Compartment_Size" prefWidth="75.0" text="Compartment Size" />
            </columns>
        </TableView>
        <Button layoutX="100.0" layoutY="328.0" mnemonicParsing="false" onAction="#show" text="Show Details" />
        <Button layoutX="390.0" layoutY="328.0" mnemonicParsing="false" onAction="#backToDash" text="Back To Dashboard" />
      <Button mnemonicParsing="false" onAction="#deleteSelectedRow" text="Delete" />
    </VBox>

</AnchorPane>
